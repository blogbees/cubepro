	<parameter name="fdate" class="java.lang.String"/>
	<parameter name="tdate" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     school_details.`school_id` AS school_id,
     school_details.`school_name` AS school_name,
     school_details.`address1` AS address1,
     school_details.`address2` AS address2,
     school_details.`phoneNo1` AS phoneNo1,
     school_details.`website` AS website,
     school_details.`phoneNo2` AS phoneNo2,
     school_details.`email` AS email,
     school_details.`faxNo` AS faxNo,
     school_details.`logo` AS logo,
     transaction_details.`trans_desc` AS trans_desc,
     transaction_details.`amount` AS amount,
     transaction_details.`trans_type` AS trans_type,
     transaction_details.`payment_type` AS payment_type,
     (prev_cash_bal+prev_bank_bal)open_balance,
     (prev_cash_bal+prev_bank_bal)open_balance,
     (prev_cash_bal+prev_bank_bal)open_balance,
     (tot_cash_bal+tot_bank_bal)close_balance,
     tot_cash_bal,
     tot_bank_bal,
     prev_cash_bal,
     prev_bank_bal,
     sum(case when transaction_code like '%INCOME%' then  abs(transaction_details.`amount`) else 0 end) as income,
     sum(case when transaction_code like '%EXPENSE%' then  abs(transaction_details.`amount`) else 0 end) as expence
FROM
     `transaction_details` transaction_details,
     `school_details` school_details,
     (SELECT SUM(CASE WHEN TRANSACTION_CODE = 'CASH INCOME' THEN abs(AMOUNT) ELSE 0 END) - SUM(CASE WHEN TRANSACTION_CODE = 'CASH EXPENSE' THEN abs(AMOUNT) ELSE 0 END) tot_cash_bal FROM transaction_details t) tot_cash_bal,
     (SELECT SUM(CASE WHEN TRANSACTION_CODE = 'BANK INCOME' THEN abs(AMOUNT) ELSE 0 END) - SUM(CASE WHEN TRANSACTION_CODE = 'BANK EXPENSE' THEN abs(AMOUNT) ELSE 0 END) tot_bank_bal FROM transaction_details t) tot_bank_bal,
     (SELECT SUM(CASE WHEN TRANSACTION_CODE = 'CASH INCOME' AND str_to_date(trans_date,'%d/%m/%Y') < str_to_date($P{fdate},'%d/%m/%Y') THEN abs(AMOUNT) ELSE 0 END) - SUM(CASE WHEN TRANSACTION_CODE = 'CASH EXPENSE' AND str_to_date(trans_date,'%d/%m/%Y') < str_to_date($P{fdate},'%d/%m/%Y') THEN abs(AMOUNT) ELSE 0 END) prev_cash_bal FROM transaction_details t) prev_cash_bal,
     (SELECT SUM(CASE WHEN TRANSACTION_CODE = 'BANK INCOME' AND str_to_date(trans_date,'%d/%m/%Y') < str_to_date($P{fdate},'%d/%m/%Y') THEN abs(AMOUNT) ELSE 0 END) - SUM(CASE WHEN TRANSACTION_CODE = 'BANK EXPENSE' AND str_to_date(trans_date,'%d/%m/%Y') < str_to_date($P{fdate},'%d/%m/%Y') THEN abs(AMOUNT) ELSE 0 END)  prev_bank_bal FROM transaction_details t) prev_bank_bal

WHERE
str_to_date(trans_date,'%d/%m/%Y') >= str_to_date($P{fdate},'%d/%m/%Y')
and str_to_date(trans_date,'%d/%m/%Y') <= str_to_date($P{tdate},'%d/%m/%Y')
group by
     transaction_details.`trans_desc` ,
     transaction_code]]>
	</queryString>
