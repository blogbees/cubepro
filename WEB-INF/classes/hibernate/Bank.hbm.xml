<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cubepro.bank.Bank" table="bank" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="bankName" type="java.lang.String">
            <column name="bank_name" length="45" >
            </column>
        </property>
        <property name="account" type="java.lang.String">
            <column name="acc_no" length="45" >
            </column>
        </property>
        <property name="branch" type="java.lang.String">
            <column name="branch" length="45" >
            </column>
        </property>        
        <property name="status" type="java.lang.String">
            <column name="status" length="45" >
            </column>
        </property>

    </class>
</hibernate-mapping>
