<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cubepro.menu.Menus" table="menus">
		<id name="menuId" type="java.lang.Integer">
			<column name="menu_id" />
			<generator class="assigned" />
		</id>
		<property name="menuName" type="java.lang.String">
			<column name="menu_name" length="145" ></column>
		</property>
		<property name="menuDesc" type="java.lang.String">
			<column name="menu_desc" length="145" ></column>
		</property>
	</class>
</hibernate-mapping>