<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cubepro.homework.HomeWork" table="homework">
		<id name="homeWorkId" type="java.lang.Integer">
			<column name="hw_id" />
			<generator class="assigned" />
		</id>
		<property name="homeWorkName" type="java.lang.String">
			<column name="hw_name" length="100"></column>
		</property>
		<property name="homeWorkDesc" type="java.lang.String">
			<column name="hw_desc" length="200"></column>
		</property>
	</class>
</hibernate-mapping>