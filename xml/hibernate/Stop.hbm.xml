<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cubepro.transport.Stop" table="stop_master">
		<id name="stopId" type="java.lang.Integer">
			<column name="stop_id" />
			<generator class="assigned" />
		</id>
		<property name="stopName" type="java.lang.String">
			<column name="stop_name" length="100"></column>
		</property>
		<property name="stopDesc" type="java.lang.String">
			<column name="stop_desc" length="100"></column>
		</property>
	</class>
</hibernate-mapping>
